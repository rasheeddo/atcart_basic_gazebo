<launch>
  <arg
    name="model" />
  <param
    name="robot_description"
    textfile="$(find atcart_greenhouse_gazebo)/urdf/atcart_greenhouse_gazebo.urdf" />
  <node
    name="joint_state_publisher"
    pkg="joint_state_publisher"
    type="joint_state_publisher" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find atcart_greenhouse_gazebo)/urdf.rviz" />
</launch>